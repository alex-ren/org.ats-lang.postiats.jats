conats_mutex_acquire(sys_tid) = 
  [sys_cur_tid == sys_tid] 
  sys_mutex_acquire(sys_tid, sys_gstack.frame_get(sys_tid, 0, 0));

conats_mutex_release(sys_tid) = 
  [sys_cur_tid == sys_tid] 
  sys_mutex_release (sys_tid, sys_gstack.frame_get(sys_tid, 0, 0));

